unit FPaiIB;

interface

uses
  Windows, Messages, SysUtils, Classes, Graphics,
  Controls, vcl.Forms, Dialogs;

type
  TFrmPaiIB = class(TForm)
    procedure FormClose(Sender: TObject; var Action: TCloseAction);
  private
    { Private declarations }
  public
    { Public declarations }
  protected
  end;

var
  FrmPaiIB: TFrmPaiIB;

implementation

uses uSistemaIB;

{$R *.DFM}

procedure TFrmPaiIB.FormClose(Sender: TObject; var Action: TCloseAction);
begin
  Action := caFree;
end;

end.
